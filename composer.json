{
    "name": "graphaware/neo4j-bolt",
    "description": "Neo4j Bolt Binary Protocol PHP Driver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Willemsen",
            "email": "christophe@graphaware.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "graphaware/neo4j-common": "^3.4",
        "myclabs/php-enum": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/stopwatch": "^5.1@dev",
        "behat/behat": "~3.0.4"
    },
    "autoload": {
      "psr-4": {
          "GraphAware\\Bolt\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphAware\\Bolt\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "minimum-stability": "dev"
}
